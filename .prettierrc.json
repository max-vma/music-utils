{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^vue", "^@[^/]+", "<THIRD_PARTY_MODULES>", "^@/", "^[./]"],
  "semi": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSameLine": true,
  "singleAttributePerLine": true,
  "emptyLineBetweenRules": true
}
